{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "FilePath": ".\\Logs\\deployment-{Date}.log"
  },
  "DeploymentSettings": {
    "ConfigurationPath": "config.json",
    "EnableDetailedLogging": true,
    "MaxConcurrentOperations": 3
  }
}
